{
  "name": "@vtaits/react-form-schema-base-ui",
  "version": "1.0.0-alpha.1",
  "description": "Structure of base ui-components to construct complex forms using @vtaits/form-schema",
  "files": [
    "dist",
    "fields"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "react-hook-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/form-schema.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn build:js && yarn build:proxy_directories",
    "build:js": "tsup",
    "build:proxy_directories": "exports-fallback -gf",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply-unsafe",
    "format": "biome format src --write",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src",
    "test:unit:watch": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@storybook/addon-docs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/react-is": "^18.2.4",
    "@vitest/coverage-v8": "^1.4.0",
    "exports-fallback": "^1.0.1",
    "happy-dom": "^14.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-integration-test-engine": "^1.4.0",
    "react-is": "^18.2.0",
    "react-select": "^5.8.0",
    "react-shallow-search": "^2.2.0",
    "react-test-engine": "^1.1.1",
    "react-test-engine-vitest": "^1.0.1",
    "react-test-renderer": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "5.4.3",
    "vitest": "^1.4.0"
  }
}
