{
  "name": "@vtaits/form-schema",
  "version": "1.0.0",
  "description": "Serialization and parsing form values by schema",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply-unsafe",
    "format": "biome format src --write",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src",
    "test:unit:watch": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "dependencies": {
    "@types/is-promise": "^2.2.0",
    "is-promise": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@vitest/coverage-v8": "^0.34.6",
    "tsup": "^7.2.0",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  }
}
