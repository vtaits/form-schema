{
  "name": "@vtaits/form-schema",
  "version": "0.3.0-alpha.0",
  "description": "Serialization and parsing form values by schema",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@types/is-promise": "^2.2.0",
    "is-promise": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@types/jest": "^28.1.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "tsup": "^6.4.0",
    "typescript": "4.3.5"
  }
}
