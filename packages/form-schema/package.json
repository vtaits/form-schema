{
  "name": "@vtaits/form-schema",
  "version": "0.3.0-alpha.1",
  "description": "Serialization and parsing form values by schema",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "dependencies": {
    "@types/is-promise": "^2.2.0",
    "is-promise": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "5.0.4"
  }
}
