{
  "name": "@vtaits/form-schema",
  "version": "0.1.0",
  "description": "Serialization and parsing form values by schema",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "/es",
    "/lib"
  ],
  "keywords": [
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\",\"src/**/__stories__\",\"src/**/__fixtures__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\",\"src/**/__stories__\",\"src/**/__fixtures__\"",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2"
  }
}
