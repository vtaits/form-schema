{
  "name": "@vtaits/react-form-schema-ui-mui-testing-library",
  "version": "1.0.0-alpha.0",
  "description": "Helpers for @testing-library/react to test @vtaits/react-form-schema-ui-mui",
  "files": [
    "dist",
    "fields"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "react-hook-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/form-schema.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn build:js",
    "build:js": "tsup",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test": "yarn lint && yarn test:ts",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.0.4",
    "@vtaits/form-schema": "^2.3.0-alpha.5",
    "@vtaits/react-hook-form-schema": "^1.2.0-alpha.8",
    "exports-fallback": "^1.0.1",
    "happy-dom": "^14.12.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.2.3",
    "typescript": "5.4.5",
    "vitest": "^2.0.4"
  }
}
