{
  "name": "@vtaits/react-final-form-schema",
  "version": "0.3.0-alpha.3",
  "description": "Integration of react-final-form and @vtaits/form-schema",
  "main": "lib/core/index.js",
  "module": "es/core/index.js",
  "typings": "ts/core/index.d.ts",
  "files": [
    "/es",
    "/fields",
    "/lib",
    "/ts"
  ],
  "keywords": [
    "react",
    "final-form",
    "react-final-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf es lib ts",
    "build": "yarn build:cjs && yarn build:es && yarn build:ts",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --root-mode upward --extensions '.ts,.tsx' src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel --root-mode upward --extensions '.ts,.tsx' src --out-dir es --ignore \"src/**/__tests__\"",
    "build:ts": "tsc --declaration",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-final-form": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@vtaits/form-schema": "^0.3.0-alpha.0",
    "is-promise": "^4.0.0",
    "react-async-hook": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "final-form": "^4.20.7",
    "final-form-arrays": "^3.0.2",
    "jest": "^28.1.3",
    "react": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.3",
    "react-select": "^5.4.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.7",
    "typescript": "4.3.5"
  }
}
