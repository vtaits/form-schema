{
  "name": "@vtaits/react-final-form-schema",
  "version": "0.1.0",
  "description": "Integration of react-final-form and @vtaits/form-schema",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "/es",
    "/lib"
  ],
  "keywords": [
    "react",
    "final-form",
    "react-final-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\",\"src/**/__stories__\",\"src/**/__fixtures__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\",\"src/**/__stories__\",\"src/**/__fixtures__\"",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "^16.8.0",
    "react-final-form": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@vtaits/form-schema": "^0.1.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "final-form": "^4.19.1",
    "final-form-arrays": "^3.0.2",
    "react": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-final-form-arrays": "^3.1.1",
    "react-select": "^3.1.0"
  }
}
