{
  "name": "@vtaits/react-final-form-schema",
  "version": "0.3.0-alpha.4",
  "description": "Integration of react-final-form and @vtaits/form-schema",
  "main": "lib/core/index.js",
  "module": "es/core/index.js",
  "typings": "ts/core/index.d.ts",
  "files": [
    "/es",
    "/fields",
    "/lib",
    "/ts"
  ],
  "exports": {
    ".": {
      "import": "./es/core/index.js",
      "require": "./lib/core/index.js",
      "types": "./ts/core/index.d.ts"
    },
    "./fields/dynamic": {
      "require": "./lib/fields/dynamic/index.js",
      "import": "./es/fields/dynamic/index.js",
      "types": "./ts/fields/dynamic/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "final-form",
    "react-final-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf es lib ts",
    "build": "yarn build:cjs && yarn build:es && yarn build:ts && exports-fallback -gf",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --root-mode upward --extensions '.ts,.tsx' src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel --root-mode upward --extensions '.ts,.tsx' src --out-dir es --ignore \"src/**/__tests__\"",
    "build:ts": "tsc --declaration",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-final-form": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@vtaits/form-schema": "^0.3.0-alpha.0",
    "is-promise": "^4.0.0",
    "react-async-hook": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "exports-fallback": "^1.0.0",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.1.0",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-select": "^5.7.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^28.0.8",
    "typescript": "4.9.5"
  }
}
