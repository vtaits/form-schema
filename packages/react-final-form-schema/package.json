{
  "name": "@vtaits/react-final-form-schema",
  "version": "0.3.0-alpha.5",
  "description": "Integration of react-final-form and @vtaits/form-schema",
  "files": [
    "dist",
    "fields"
  ],
  "main": "./dist/core.js",
  "module": "./dist/esm/core.js",
  "typings": "dist/core.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/core.js",
      "require": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./fields/dynamic": {
      "require": "./dist/fields_dynamic.js",
      "import": "./dist/esm/fields_dynamic.js",
      "types": "./dist/fields_dynamic.d.ts"
    }
  },
  "keywords": [
    "react",
    "final-form",
    "react-final-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn build:js && yarn build:proxy_directories",
    "build:js": "tsup --entry.core src/core/index.ts --entry.fields_dynamic src/fields/dynamic/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "build:proxy_directories": "exports-fallback -gf",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src",
    "test:unit:watch": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-final-form": "^6.0.0"
  },
  "dependencies": {
    "@vtaits/form-schema": "^0.3.0-alpha.0",
    "@vtaits/react-required-context": "^1.0.0",
    "is-promise": "^4.0.0",
    "react-async-hook": "^4.0.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/react": "^18.2.6",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "exports-fallback": "^1.0.1",
    "final-form": "^4.20.9",
    "final-form-arrays": "^3.1.0",
    "react": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-is": "^18.2.0",
    "react-select": "^5.7.3",
    "react-test-engine": "^1.1.0",
    "react-test-renderer": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "5.0.4",
    "vitest": "^0.34.6"
  }
}
