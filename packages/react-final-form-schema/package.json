{
  "name": "@vtaits/react-final-form-schema",
  "version": "0.2.5",
  "description": "Integration of react-final-form and @vtaits/form-schema",
  "main": "lib/core/index.js",
  "module": "es/core/index.js",
  "typings": "ts/core/index.d.ts",
  "files": [
    "/es",
    "/fields",
    "/lib",
    "/ts"
  ],
  "keywords": [
    "react",
    "final-form",
    "react-final-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": "git@github.com:vtaits/form-schema.git",
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf es lib ts",
    "build": "yarn build:cjs && yarn build:es && yarn build:ts",
    "build:cjs": "cross-env BABEL_ENV=cjs babel --root-mode upward --extensions '.ts,.tsx' src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel --root-mode upward --extensions '.ts,.tsx' src --out-dir es --ignore \"src/**/__tests__\"",
    "build:ts": "tsc --declaration",
    "test:ts": "tsc -p ./tsconfig.eslint.json",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "final-form": "^4.0.0",
    "react": "^16.14.0 || ^17.0.0",
    "react-final-form": "^6.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@vtaits/form-schema": "^0.2.4",
    "is-promise": "^4.0.0",
    "react-async-hook": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "final-form": "^4.20.6",
    "final-form-arrays": "^3.0.2",
    "jest": "^27.5.1",
    "react": "^17.0.2",
    "react-final-form": "^6.5.8",
    "react-final-form-arrays": "^3.1.3",
    "react-select": "^5.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "4.3.5"
  }
}
