{
  "name": "@vtaits/react-hook-form-schema",
  "version": "1.0.0",
  "description": "Integration of react-hook-form and @vtaits/form-schema",
  "files": [
    "dist",
    "fields"
  ],
  "main": "./dist/core.js",
  "module": "./dist/esm/core.js",
  "typings": "dist/core.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/core.js",
      "require": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./fields/dynamic": {
      "require": "./dist/fields_dynamic.js",
      "import": "./dist/esm/fields_dynamic.js",
      "types": "./dist/fields_dynamic.d.ts"
    }
  },
  "keywords": [
    "react",
    "react-hook-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/form-schema.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn build:js && yarn build:proxy_directories",
    "build:js": "tsup --entry.core src/core/index.ts --entry.fields_dynamic src/fields/dynamic/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "build:proxy_directories": "exports-fallback -gf",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --apply-unsafe",
    "format": "biome format src --write",
    "test:unit": "vitest run --coverage.enabled --coverage.all --coverage.include=src",
    "test:unit:watch": "vitest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-hook-form": "^7.0.0"
  },
  "dependencies": {
    "@vtaits/form-schema": "^2.0.0",
    "is-promise": "^4.0.0",
    "use-latest": "^1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@testing-library/react": "^14.1.0",
    "@types/lodash": "^4.14.201",
    "@types/react": "^18.2.37",
    "@vitest/coverage-v8": "^0.34.6",
    "exports-fallback": "^1.0.1",
    "happy-dom": "^12.10.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-integration-test-engine": "^1.3.0",
    "react-is": "^18.2.0",
    "react-select": "^5.8.0",
    "react-test-engine": "^1.1.1",
    "react-test-engine-vitest": "^1.0.0",
    "react-test-renderer": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  }
}
