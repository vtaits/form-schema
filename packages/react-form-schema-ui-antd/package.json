{
  "type": "module",
  "name": "@vtaits/react-form-schema-ui-antd",
  "version": "1.0.0-alpha.11",
  "description": "Antd fields for @vtaits/form-schema",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "react",
    "react-hook-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/form-schema.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test:unit": "",
    "test": "bun run lint && bun run test:ts",
    "prepare": "bun run clean && bun run build"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.3.7",
    "@vtaits/react-form-schema-base-ui": "^1.1.0",
    "antd": "^5.12.3",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@biomejs/biome": "^2.2.2",
    "@storybook/react-vite": "^9.1.3",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vtaits/form-schema": "^3.0.2",
    "@vtaits/react-form-schema-base-ui": "^1.1.0",
    "@vtaits/react-hook-form-schema": "^2.2.0",
    "antd": "5.27.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "dayjs": "^1.11.13"
  }
}
