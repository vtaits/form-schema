{
  "name": "@vtaits/react-form-schema-ui-antd",
  "version": "1.0.0-alpha.9",
  "description": "Antd fields for @vtaits/form-schema",
  "files": [
    "dist",
    "fields"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "react",
    "react-hook-form",
    "form",
    "schama",
    "serialization",
    "parsing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vtaits/form-schema.git"
  },
  "author": "Vadim Taits <taizvadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "bun run build:js && bun run build:proxy_directories",
    "build:js": "tsup",
    "build:proxy_directories": "exports-fallback -gf",
    "test:ts": "tsc",
    "lint": "biome check src",
    "lint:fix": "biome check src --write --unsafe",
    "format": "biome format src --write",
    "test:unit": "",
    "test": "bun run lint && bun run test:ts",
    "prepare": "bun run clean && bun run build"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.3.7",
    "@vtaits/react-form-schema-base-ui": "^1.0.0-alpha.11",
    "antd": "^5.12.3",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.6.0",
    "@biomejs/biome": "^1.9.4",
    "@storybook/addon-docs": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vtaits/form-schema": "^2.3.0-alpha.11",
    "@vtaits/react-form-schema-base-ui": "^1.0.0-alpha.11",
    "@vtaits/react-hook-form-schema": "^1.2.0-alpha.15",
    "antd": "5.23.3",
    "exports-fallback": "^1.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.6",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "dayjs": "^1.11.13"
  }
}
